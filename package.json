{
  "name": "website-schedler-co",
  "description": "Curriculum vitae (CV) | Alexander Schedler: Internet application engineering, development and management. Javascript (Frontend and Backend), HTML, CSS, Node, CouchDB",
  "version": "7.0.0",
  "license": "CC-BY-NC-ND",
  "repository": {
    "type": "git",
    "url": "https://github.com/kulturpessimist/schedler.co.git"
  },
  "browserslist": "> 0.2%",
  "scripts": {
    "dev": "npm run start",
    "start": "npm run build:rev -- ./src/version.json; parcel src/index.html --open",
    "clean:build": "rm -rf dist/",
    "build:rev": "node rev.js",
    "build:pages": "node src/scripts/build-pages.js",
    "build": "npm run clean:build; npm run build:rev -- ./src/version.json; npm run build:pages; parcel build src/index.html",
    "build:detailed": "npm run build -- --public-url ./ --no-cache --log-level 4 --detailed-report 10",
    "serve": "serve dist/"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "navigo": "^8.11.1",
    "petite-vue": "^0.4.1",
    "rlite-router": "^2.0.3",
    "rough-notation": "0.5.1"
  },
  "devDependencies": {
    "asciify-image": "^0.1.8",
    "isomorphic-git": "^1.13.1",
    "parcel": "^2.3.2",
    "prettier": "2.5.1",
    "serve": "^13.0.2"
  }
}
